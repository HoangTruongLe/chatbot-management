$(document).on("ajax:success", "#new_tag", function(ev, data, xhr, status){
  $("#form_tag").replaceWith("<%= j render partial: 'form_new', locals: {tag: @tag}%>")
  $('#tagNewModal').modal()
});

$(document).on("ajax:error", "#new_tag", function(e, xhr, status, error){
  $("#form_tag").replaceWith("<p>ERROR</p>")
});